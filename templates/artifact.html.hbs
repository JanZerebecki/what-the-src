{{#*inline "title"}}
{{chksum}} - What the src?!
{{/inline}}

{{#*inline "page"}}
<h1 class="word-wrap">{{artifact.chksum}}</h1>

{{~#if alias}}
<div class="alias word-wrap">
<h2>Alias</h2>
<p>Detected
<a href="/artifact/{{artifact.chksum}}">canonical page</a>
for alias
{{~#if alias.reason}}
 ({{alias.reason}})
{{~/if}}
: <code>{{alias.alias_from}}</code></p>
</div>
{{/if}}

{{~#if refs}}
<h2>Build input of</h2>
<ul>
{{#each refs}}
<li class="word-wrap">
<b>{{this.display_vendor}}:</b>
{{#if this.href}}
<a href="{{this.href}}">
{{/if}}
{{this.package}}
{{this.version}}
{{~#if this.href}}
</a>
{{/if}}
{{#if this.filename}}
({{this.filename}})
{{/if}}
<a href="/artifact/{{this.chksum}}">{{this.chksum}}</a>
</li>
{{/each}}
</ul>
{{/if}}

{{#if artifact.files}}
<h2>Content</h2>
<pre id="files">
{{#each artifact.files}}
{{#if this.digest}}
{{this.digest}}  {{this.path}}
{{else}}
                                                                         {{this.path}}
{{/if}}
{{/each}}
</pre>
{{/if}}
{{/inline}}

{{> base.html.hbs }}
